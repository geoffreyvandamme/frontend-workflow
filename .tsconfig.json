{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "typeRoots": ["./src/js/typings"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "lib": ["dom", "es7", "dom.iterable"]
  }
}
